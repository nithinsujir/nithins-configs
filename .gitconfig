[user]
	name = Nithin Nayak Sujir
	email = nsujir@google.com
[color]
	ui = auto
[core]
	excludesfile = ~/.gitignore
[push]
	default = current
[credential]
	helper = cache --timeout=999999
[merge]
	tool = kdiff3
	tool = kdiff3
[alias]
	l = log --pretty=format:'%C(yellow)%h%C(white) %an <%ae> %>|(80)%ai%Creset%n%C(bold)%w(0,4,4)%s%C(reset)%n%+b'
	ll = log --stat
	lg = log
	lp = log -p
