# Upgrade txos
upg() {
	txos=$1
	target=$2
	/auto/e2e/bin/UpgradeTools.py install --rpmpath $txos $target
}

# Upgrade to latest <branch>
upgb() {
	local branch=$1 target=$2
	/auto/e2e/bin/UpgradeTools.py install --branch $branch $target
}

# Upgrade to latest default
upgd() {
	local target=$1
	/auto/e2e/bin/UpgradeTools.py install --branch default $target
}

upgt() {
	if [[ -z $1 || -z $2 ]]; then
		echo "$0 <target> <rpm>"
		return -1
	fi

	target=$1
	rpm=$(basename $2)

	/bin/ping -c 1 -W 3 ${target}a 2>&1 > /dev/null
	if [[ $? -ne 0 ]]; then
		echo "${target}a not accessible"
		return 1
	fi

	/bin/ping -c 1 -W 3 ${target}b 2>&1 > /dev/null
	if [[ $? -ne 0 ]]; then
		echo "${target}a not accessible"
		return 1
	fi

	echo "scp'ing $rpm"
	$SCP $TOPDIR/distro_bld/local_rpms/RPMS/x86_64/$rpm root@${target}a:/var/releases/upload
	if [[ $? -ne 0 ]]; then
		echo "scp failed to controller A"
		return 1
	fi

	$SCP $TOPDIR/distro_bld/local_rpms/RPMS/x86_64/$rpm root@${target}b:/var/releases/upload
	if [[ $? -ne 0 ]]; then
		echo "scp failed to controller B"
		return 1
	fi

	echo "Stopping services"
	$SSH root@${target}a "/sbin/service postgresql-perf stop"
	$SSH root@${target}a "/sbin/service txos stop"
	$SSH root@${target}b "/sbin/service postgresql-perf stop"
	$SSH root@${target}b "/sbin/service txos stop"

	echo "Starting upgrade_txos on A"
	$SSH root@${target}a "/usr/local/tintri/bin/upgrade_txos -f -F -v -r /var/releases/upload/$rpm"
	if [[ $? -ne 124 ]]; then
		echo "Upgrade failed on controller A"
	else
		$SSH root@${target}a "reboot"
	fi

	echo "Starting upgrade_txos on B"
	$SSH root@${target}b "/usr/local/tintri/bin/upgrade_txos -f -F -v -r /var/releases/upload/$rpm"
	if [[ $? -ne 124 ]]; then
		echo "Upgrade failed on controller B"
	else
		$SSH root@${target}b "reboot"
	fi
}

